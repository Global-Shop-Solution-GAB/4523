        <string />
        <string>f.Intrinsic.Control.If(v.Caller.Hook,=,51100)</string>
        <string>Function.Intrinsic.Control.AndIf(v.Passed.SYS050,=,"OLG002")</string>
        <string>	'Get heat # from user.</string>
        <string>	f.Intrinsic.String.Build("select distinct Heat from item_history where JOB = '{0}' and suffix = '{1}' and code_transaction = 'J55'",v.Passed.009000,v.Passed.009001,v.Local.sSql)</string>
