        <string>	f.ODBC.Connection!conx.OpenRecordsetRO("rst",v.Local.sSql)</string>
        <string>	f.Intrinsic.Control.If(v.ODBC.conx!rst.EOF,&lt;&gt;,True)</string>
        <string>		</string>
        <string>		'Load selections to </string>
        <string>		f.Intrinsic.Control.DoUntil(v.ODBC.conx!rst.EOF,=,true)</string>
        <string>			f.Intrinsic.Control.If(v.ODBC.conx!rst.FieldValTrim!HEAT,&lt;&gt;,"")</string>
        <string>				gui.SS_Form1.ddlHeat.AddItem(v.ODBC.conx!rst.FieldValTrim!HEAT)</string>
        <string>				f.Intrinsic.Math.Add(1,v.Local.iTemp,v.Local.iTemp)</string>
        <string>				v.Local.bExists.Set(True)</string>
        <string>				v.Global.sHeat.Set(v.ODBC.conx!rst.FieldValTrim!HEAT)</string>
        <string>			f.Intrinsic.Control.EndIf</string>
        <string>			</string>
        <string>			f.ODBC.conx!rst.MoveNext</string>
        <string>		f.Intrinsic.Control.Loop	</string>
        <string>	f.Intrinsic.Control.EndIf</string>
        <string>	f.ODBC.conx!rst.Close</string>
        <string>	f.ODBC.Connection!conx.Close</string>
        <string>	</string>
        <string>	'if only ONE heat, then just assign that one and end.</string>
        <string>	f.Intrinsic.Control.If(v.Local.iTemp,=,1)</string>
        <string>	Function.Intrinsic.Control.AndIf(v.Global.sHeat.Trim,&lt;&gt;,"")</string>
